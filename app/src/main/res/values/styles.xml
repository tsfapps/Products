<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="btn_add">
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_width">22dp</item>
        <item name="android:background">@drawable/ic_add</item>
    </style>
    <style name="btn_less">
        <item name="android:layout_height">22dp</item>
        <item name="android:layout_width">22dp</item>
        <item name="android:background">@drawable/ic_remove</item>
    </style>
    <style name="iv_dashboard">
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_width">50dp</item>
        <item name="android:padding">8dp</item>

    </style>
    <style name="iv_dashboard_report">
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">40dp</item>
        <item name="android:padding">8dp</item>

    </style>

    <style name="tv_dash_label">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_marginTop">-8dp</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/colorGrey</item>
        <item name="android:textStyle">bold</item>
    </style>
    <style name="tv_cash_label">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="tv_report_months">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginStart">1dp</item>
        <item name="android:layout_marginEnd">1dp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_simple_ascent</item>
    </style>
    <style name="ll_expenses">
        <item name="android:layout_marginTop">@dimen/margin_8</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_gravity">center_horizontal</item>
    </style>
    <style name="tv_expense_label">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginStart">@dimen/margin_8</item>
        <item name="android:layout_marginEnd">@dimen/margin_8</item>
    </style>
    <style name="et_expense_label">
        <item name="android:text">0</item>
        <item name="android:layout_marginStart">@dimen/margin_8</item>
        <item name="android:layout_marginEnd">@dimen/margin_8</item>
        <item name="android:inputType">number</item>
        <item name="android:maxLength">4</item>
        <item name="android:maxLines">1</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">.4</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_et</item>

    </style>
    <style name="tv_cash_counter">
        <item name="android:gravity">center</item>
        <item name="android:text">0</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:background">@drawable/bg_tv</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="et_product_counter">
        <item name="android:gravity">center</item>
        <item name="android:hint">0</item>
        <item name="android:layout_marginTop">4dp</item>
        <item name="android:inputType">number</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:textColor">@color/colorPrimaryDark</item>
        <item name="android:background">@drawable/bg_tv</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="tv_product_list">
        <item name="android:gravity">start</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="color_product_list">
        <item name="android:gravity">center</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="iv_dash_bottom">
        <item name="android:layout_height">30dp</item>
        <item name="android:layout_width">30dp</item>
        <item name="android:layout_weight">1</item>
    </style>
    <style name="tv_report_top">
        <item name="android:layout_height">35dp</item>
        <item name="android:layout_margin">1dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">@drawable/bg_simple_dark</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textSize">8sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="btn_main">
        <item name="android:layout_marginTop">@dimen/margin_8</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_width">120dp</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:background">@drawable/bg_btn_main</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:text">Submit</item>
    </style>
    <style name="btn_main_list">
        <item name="android:layout_marginTop">@dimen/margin_8</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@drawable/bg_btn_main</item>
        <item name="android:textColor">@color/white</item>
    </style>
    <style name="et_main">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:ems">150</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:background">@drawable/bg_et</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="et_main_login">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_marginEnd">4dp</item>
        <item name="android:layout_marginStart">4dp</item>
        <item name="android:background">@drawable/bg_et</item>
        <item name="android:textColor">@color/colorBlack</item>
        <item name="android:textSize">12sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ll_main">
        <item name="android:layout_marginTop">@dimen/margin_8</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="rl_main_spinner">
        <item name="android:layout_marginTop">@dimen/margin_8</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="ll_cash_counter">
        <item name="android:padding">4dp</item>
        <item name="android:layout_marginEnd">8dp</item>
        <item name="android:layout_marginStart">8dp</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="spinner_main">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:spinnerMode">dropdown</item>
        <item name="android:layout_height">40dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="rl_spinner_main">
        <item name="android:background">@drawable/bg_simple_ascent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
    </style>

    <style name="card_dash" parent="Base.CardView">
        <item name="cardCornerRadius">4dp</item>
        <item name="cardElevation">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_margin">.5dp</item>
    </style>
    <style name="ll_dash">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>
    <style name="card_dash_report" parent="Base.CardView">
        <item name="android:layout_margin">.5dp</item>
        <item name="cardCornerRadius">4dp</item>
        <item name="cardElevation">10dp</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_weight">1</item>
    </style>

    <style name="ll_dash_report">
        <item name="android:layout_margin">.5dp</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:orientation">vertical</item>
    </style>

    <style name="tv_report">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">10sp</item>
        <item name="android:layout_marginTop">8dp</item>
        <item name="android:textColor">@color/colorGrey</item>
    </style>
    <style name="tv_detail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>


    </style>
    <style name="tv_disable">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">12sp</item>
        <item name="android:minHeight">40dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/bg_simple_ascent</item>
        <item name="android:gravity">center</item>


    </style>


    <style name="spn_item_count">
        <item name="android:layout_height">30dp</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_marginStart">@dimen/margin_32</item>
        <item name="android:layout_marginEnd">@dimen/margin_32</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:background">@drawable/bg_et</item>
        <item name="android:textSize">14sp</item>

    </style>

</resources>
